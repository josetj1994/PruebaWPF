<Window x:Class="PruebaWPF.DialogoModificarAlimento"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PruebaWPF"
        mc:Ignorable="d"
        Title="DialogoModificarAlimento" Height="450" Width="800">
    <Grid>
        <Label x:Name="caloriasMed" Content="Cambiar Cantidad Consumida:" HorizontalAlignment="Left" Margin="180,154,0,0" VerticalAlignment="Top" Width="174"/>
        <Button x:Name="Guardar" Content="Guardar" HorizontalAlignment="Left" Margin="290,223,0,0" VerticalAlignment="Top" Width="75" Click="Guardar_Click"/>
        <Button x:Name="cancelar" Content="Cancelar" HorizontalAlignment="Left" Margin="431,223,0,0" VerticalAlignment="Top" Width="75" Click="cancelar_Click"/>
        <TextBox x:Name="CambiarCantidad" HorizontalAlignment="Left" Height="23" Margin="376,154,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="nombreAlimento" HorizontalAlignment="Left" Height="23" Margin="376,103,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label Content="Alimento a cambiar:" HorizontalAlignment="Left" Margin="238,103,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
